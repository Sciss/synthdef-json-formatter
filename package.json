{
  "name": "synthdef-json-formatter",
  "description": "formatter for SuperCollider Synth Definition File Format",
  "version": "1.0.0",
  "author": "Nao Yonamine <mohayonao@gmail.com>",
  "bugs": {
    "url": "https://github.com/mohayonao/synthdef-json-formatter/issues"
  },
  "dependencies": {
    "lodash.topairs": "^4.3.0",
    "lodash.unzip": "^3.4.0",
    "lodash.zip": "^4.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "eater": "^3.2.0",
    "eatest": "^1.0.0",
    "eslint": "^3.19.0",
    "npm-run-all": "^4.0.2",
    "nyc": "^11.0.1"
  },
  "engines": {
    "node": ">= 4.0.0"
  },
  "files": [
    "package.json",
    "README.md",
    "lib"
  ],
  "homepage": "https://github.com/mohayonao/synthdef-json-formatter/",
  "keywords": [
    "synthdef"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mohayonao/synthdef-json-formatter.git"
  },
  "scripts": {
    "build": "npm-run-all build:*",
    "build:to5": "babel --out-dir=lib src",
    "clean": "rm -rf lib coverage .nyc_output npm-debug.log",
    "cover": "BABEL_ENV=coverage nyc --reporter text --reporter html eater",
    "lint": "eslint src test",
    "postversion": "git push && git push --tags && npm run clean",
    "prepublish": "npm-run-all clean lint test build",
    "preversion": "npm-run-all clean lint test",
    "test": "eater",
    "travis": "npm-run-all lint test"
  }
}
